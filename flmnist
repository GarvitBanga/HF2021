        "from sklearn.utils import shuffle\n",
        "x_train, y_train= shuffle(x_train, y_train, random_state=0)\n",
        "X=[]\n",
        "Y=[]\n",
        "j=0\n",
        "for i in range(20):\n",
        "  X.append(x_train[j:j+3000])\n",
        "  Y.append(y_train[j:j+3000])\n",
        "  j+=3000\n",
        "\n"
      ],
      "execution_count": null,
      "outputs": []
